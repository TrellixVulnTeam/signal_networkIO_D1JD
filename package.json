{
  "name": "signal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "ng:serve": "ng serve -o -a=intercept -p=5000",
    "dev": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update check",
    "update:skip": "./node_modules/.bin/nx update skip",
    "postinstall": "./node_modules/.bin/nx postinstall && ng build --app=intercept",
    "ctrl": "ng serve -o -a=ctrl -p=4202",
    "rcvr": "ng serve -o -a=intercept -p=4201",
    "interface": "ng serve -o -a=interface -p=4203",
    "sgnl": "npm run ctrl & npm run rcvr",
    "server": "tsc server/index.ts && nodemon server",
    "build.desktop": "tsc ./apps/interface/src/desktop",
    "build.intercept": "ng build --app=intercept",
    "build.interface": "ng build --app=interface",
    "electron:tsc": "tsc native/intercept.ts",
    "electron:intercept": "npm run build.intercept && npm run electron:tsc && electron ./native/intercept.js",
    "electron:interface": "npm run electron:tsc && electron ./native/interface.js",
    "electron:serve": "wait-on http-get://localhost:5000/ && npm run electron:tsc && electron ./native/intercept.js --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "watch": "tsc -w",
    "desktop": "ng build -a=interface --prod && npm run build.desktop && electron ./apps/interface/src/desktop.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.6",
    "@angular/cdk": "^5.2.4",
    "@angular/cli": "1.7.1",
    "@angular/common": "5.2.6",
    "@angular/compiler": "5.2.6",
    "@angular/core": "5.2.6",
    "@angular/flex-layout": "^5.0.0-beta.14",
    "@angular/forms": "5.2.6",
    "@angular/platform-browser": "5.2.6",
    "@angular/platform-browser-dynamic": "5.2.6",
    "@angular/router": "5.2.6",
    "@ngrx/effects": "5.1.0",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.1.0",
    "@ngrx/store-devtools": "5.1.0",
    "@nrwl/nx": "0.8.0",
    "@types/express": "^4.11.1",
    "@types/socket.io": "^1.4.32",
    "@types/socket.io-client": "^1.4.32",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.4.1",
    "electron-builder": "^20.0.5",
    "electron-reload": "^1.2.2",
    "electron-store": "^1.3.0",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "moment": "^2.21.0",
    "ng-socket-io": "^0.2.4",
    "ngx-electron": "^1.0.4",
    "ngx-webstorage": "^2.0.1",
    "os": "^0.1.1",
    "request-promise": "^4.2.2",
    "rxjs": "^5.5.6",
    "simple-line-icons": "^2.4.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "wait-on": "^2.1.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.6",
    "@angular/language-service": "5.2.6",
    "@nrwl/schematics": "0.8.0",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.103",
    "angular2-hotkeys": "^2.0.4",
    "codelyzer": "^4.0.1",
    "devtron": "^1.4.0",
    "electron-packager": "^11.2.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.10.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  },
  "engines": {
    "node": "9.10.0"
  }
}
